          <BeanShellPreProcessor guiclass="TestBeanGUI" testclass="BeanShellPreProcessor" testname="BeanShell PreProcessor " enabled="true">
            <boolProp name="resetInterpreter">true</boolProp>
            <stringProp name="parameters"></stringProp>
            <stringProp name="filename"></stringProp>
            <stringProp name="script">
String currentDateString =(String) vars.get(&quot;TODAY&quot;);
currentDateString.trim();
System.out.println(&quot;currentDateString.length()-&gt;&quot; + currentDateString.length() + ctx.getThreadNum());

int  currentDate=Integer.parseInt(currentDateString.substring(7,9));
//System.out.println(&quot;currentDate &quot; + currentDate );
int currentMonth =Integer.parseInt(currentDateString.substring(5,7));
int currentYear =Integer.parseInt(currentDateString.substring(1,5));



int nextDay=0;
int nextMonth=0;
int nextYear =0;

String nextMonthString=&quot;&quot;;
String nextDateString=&quot;&quot;;

String tomorrow = &quot;&quot;;


if (currentDate == 31 &amp;&amp;  currentMonth==12) {
	nextDay =1;
	nextMonth=1;
	nextYear = currentYear+ 1;
} else if (currentDate == 31) {
	nextDay =1;
	nextMonth= currentMonth + 1;
	nextYear = currentYear;
} else if  (currentDate == 30 &amp;&amp; ( currentMonth==4 ||  currentMonth==6  ||  currentMonth==9 ||  currentMonth==11)) {
	nextDay =1;
	nextMonth= currentMonth + 1;
	nextYear = currentYear;
} else if  ( currentMonth==2  &amp;&amp; currentDate==29 &amp;&amp; ( currentYear%4 ==0)) {
	nextDay = 1;
	nextMonth= currentMonth + 1;
	nextYear = currentYear;
} else if  ( currentMonth==2  &amp;&amp; currentDate==28 &amp;&amp; ( currentYear%4 &gt; 0)) {
	nextDay = 1;
	nextMonth= currentMonth + 1;
	nextYear = currentYear;
} else {
     	nextDay =currentDate+ 1;
	nextMonth= currentMonth;
	nextYear = currentYear;
}


if ( nextDay &lt; 10) {
       	 nextDateString = &quot;0&quot; + nextDay;	
}  else {
	nextDateString = &quot;&quot; +  nextDay;

}

if ( nextMonth &lt; 10) { 
      	nextMonthString = &quot;0&quot; + nextMonth;	
} else {
	nextMonthString =&quot;&quot; +  nextMonth;
}

tomorrow = nextDateString + &quot;/&quot; + nextMonthString + &quot;/&quot; + nextYear;
vars.put(&quot;tomorrow&quot;, tomorrow);

System.out.println(&quot; Next Date-- &quot; + tomorrow );</stringProp>
